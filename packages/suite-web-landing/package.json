{
    "name": "@trezor/suite-web-landing",
    "version": "21.9.0",
    "private": true,
    "scripts": {
        "type-check": "tsc --project tsconfig.json",
        "type-check:watch": "yarn type-check -- --watch",
        "lint": "eslint . --ext .tsx,.ts",
        "copy-files": "yarn workspace @trezor/suite-data copy-static-files",
        "dev": "rimraf ./build && yarn workspace @trezor/suite-build run dev:landing",
        "build": "rimraf ./build && yarn workspace @trezor/suite-build run build:landing"
    },
    "dependencies": {
        "@trezor/components": "1.0.0",
        "@sentry/browser": "^6.10.0",
        "@sentry/integrations": "^6.10.0",
        "worker-loader": "^3.0.7",
        "styled-components": "5.1.1",
        "react-helmet": "^6.1.0",
        "react-router-dom": "^5.2.0"
    },
    "devDependencies": {
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-helmet": "^6.1.0",
        "@types/react-redux": "^7.1.7",
        "is-website-vulnerable": "^1.14.6",
        "@types/react-router": "^5.1.11",
        "@types/react-router-dom": "^5.1.7"
    }
}
